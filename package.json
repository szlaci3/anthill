{
  "name": "anthill",
  "version": "1.0.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "deploy": "npm run build && npm run gh-pages",
    "dev": "npm run start:dev",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier && npm run tsc",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "openapi": "umi openapi",
    "playwright": "playwright install && playwright test",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"src/**/*\"",
    "serve": "umi-serve",
    "start": "cross-env UMI_ENV=dev umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev umi dev",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=dev umi dev",
    "start:no-ui": "cross-env UMI_UI=none UMI_ENV=dev umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=dev umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=dev umi dev",
    "test": "react-scripts test --env=jsdom",
    "test:component": "umi test ./src/components",
    "test:e2e": "node ./tests/run-tests.js"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/charts": "1.3.5",
    "@ant-design/icons": "4.7.0",
    "@ant-design/pro-descriptions": "1.10.43",
    "@ant-design/pro-form": "1.53.6",
    "@ant-design/pro-layout": "6.32.14",
    "@ant-design/pro-table": "2.63.6",
    "@testing-library/react": "12.1.5",
    "@testing-library/user-event": "^14.4.3",
    "@uiw/react-amap-map": "3.0.4",
    "@umijs/route-utils": "2.0.5",
    "amdefine": "1.0.1",
    "antd": "4.19.5",
    "array-uniq": "1.0.3",
    "btoa": "^1.2.1",
    "classnames": "2.3.1",
    "has-ansi": "2.0.0",
    "lodash": "4.17.21",
    "moment": "2.29.1",
    "omit.js": "2.0.2",
    "pinkie": "2.0.4",
    "pinkie-promise": "2.0.1",
    "rc-menu": "9.3.2",
    "rc-util": "5.18.1",
    "react": "17.0.2",
    "react-dev-inspector": "1.7.1",
    "react-dom": "17.0.2",
    "react-helmet-async": "1.2.2",
    "react-highlight-words": "0.17.0",
    "react-scripts": "^5.0.1",
    "react-use": "17.3.1",
    "react-window": "1.8.6",
    "umi": "3.5.20",
    "xss": "1.0.14"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "2.1.5",
    "@playwright/test": "1.19.1",
    "@testing-library/jest-dom": "5.16.5",
    "@types/express": "4.17.13",
    "@types/history": "4.7.11",
    "@types/jest": "26.0.24",
    "@types/lodash": "4.14.178",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "@types/react-helmet": "6.1.5",
    "@umijs/fabric": "2.10.1",
    "@umijs/openapi": "1.4.0",
    "@umijs/plugin-blocks": "2.2.2",
    "@umijs/plugin-esbuild": "1.4.1",
    "@umijs/plugin-openapi": "1.3.2",
    "@umijs/preset-ant-design-pro": "1.3.3",
    "@umijs/preset-dumi": "1.1.38",
    "@umijs/preset-react": "1.8.30",
    "@umijs/yorkie": "2.0.5",
    "carlo": "0.9.46",
    "cross-env": "7.0.3",
    "cross-port-killer": "1.4.0",
    "detect-installer": "1.0.2",
    "enzyme": "3.11.0",
    "eslint": "7.32.0",
    "express": "4.17.2",
    "gh-pages": "3.2.3",
    "jsdom-global": "3.0.2",
    "lint-staged": "10.5.4",
    "mockjs": "1.1.0",
    "netlify-cli": "^13.0.0",
    "prettier": "2.5.1",
    "puppeteer-core": "8.0.0",
    "stylelint": "13.13.1",
    "swagger-ui-react": "3.52.5",
    "umi-serve": "1.9.10"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "gitHooks": {
    "commit-msg": "fabric verify-commit"
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!antd|@ant-design|rc-.+?|@babel/runtime).+(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^@@/(.*)$": "<rootDir>/src/.umi/$1"
    }
  }
}
